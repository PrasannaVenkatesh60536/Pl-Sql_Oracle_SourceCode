{\listtext\f1 44.\tab}  ELSIF\~v_number < 20 THEN\par
{\listtext\f1 45.\tab}    dbms_output.put_line('I am smaller than 20');\par
{\listtext\f1 46.\tab}  ELSIF\~v_number < 30 THEN\par
{\listtext\f1 47.\tab}    dbms_output.put_line('I am smaller than 30');\par
{\listtext\f1 48.\tab}  ELSE\par
{\listtext\f1 49.\tab}    IF v_number IS\~NULL THEN\par
{\listtext\f1 50.\tab}      dbms_output.put_line('The number is null..');\par
{\listtext\f1 51.\tab}    ELSE\par
{\listtext\f1 52.\tab}      dbms_output.put_line('I am equal or greater than 30');\par
{\listtext\f1 53.\tab}    END\~IF;\par
{\listtext\f1 54.\tab}  END\~IF;\par
{\listtext\f1 55.\tab}END;\par

