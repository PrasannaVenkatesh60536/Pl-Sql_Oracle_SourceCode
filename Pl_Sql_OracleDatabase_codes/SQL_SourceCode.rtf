\pard 
{\listtext\f0 1.\tab}\jclisttab\tx1440\ls2\ilvl1\widctlpar\sa200\sl276\slmult1\cf2\expndtw15\f0\fs24\par
{\listtext\f0 2.\tab}\par
{\listtext\f0 3.\tab}Nested Loop\par

\pard\cbpat6 
{\listtext\f1 15.\tab}\jclisttab\tx1080\ls2\widctlpar\fi-360\cf0\expndtw0\f1\fs20 DECLARE\par
{\listtext\f1 16.\tab} v_inner NUMBER\~:= 1;\par
{\listtext\f1 17.\tab}BEGIN\par
{\listtext\f1 18.\tab} FOR v_outer IN\~1..5 LOOP\par
{\listtext\f1 19.\tab}  dbms_output.put_line(' value is : ' || v_outer );\par
{\listtext\f1 20.\tab}    v_inner := 1;\par
{\listtext\f1 21.\tab}    LOOP\par
{\listtext\f1 22.\tab}      v_inner := v_inner+1;\par
{\listtext\f1 23.\tab}      dbms_output.put_line(\lquote value is : ' || v_inner );\par
{\listtext\f1 24.\tab}      EXIT WHEN\~v_inner * v_outer >= 15;\par
{\listtext\f1 25.\tab}    END\~LOOP;\par
{\listtext\f1 26.\tab} END\~LOOP;\par
{\listtext\f1 27.\tab}END;\par

\pard 
{\listtext\f0 1.\tab}\jclisttab\tx1440\ls2\ilvl1\widctlpar\sa200\sl276\slmult1\cf2\expndtw15\f0\fs24 Continue\par

\pard\cbpat6 
{\listtext\f1 28.\tab}\jclisttab\tx1080\ls2\widctlpar\fi-360\cf0\expndtw0\f1\fs20 DECLARE\par
{\listtext\f1 29.\tab} v_inner NUMBER := 1;\par
{\listtext\f1 30.\tab}BEGIN\par
{\listtext\f1 31.\tab} FOR v_outer IN 1..10 LOOP\par
{\listtext\f1 32.\tab}  dbms_output.put_line('My outer value is : ' || v_outer );\par
{\listtext\f1 33.\tab}    v_inner := 1;\par
{\listtext\f1 34.\tab}    WHILE v_inner * v_outer < 15 LOOP\par
{\listtext\f1 35.\tab}      v_inner := v_inner + 1;\par
{\listtext\f1 36.\tab}      CONTINUE WHEN MOD(v_inner * v_outer,3) = 0;\par
{\listtext\f1 37.\tab}      dbms_output.put_line('  My inner value is : ' || v_inner );\par
{\listtext\f1 38.\tab}    END LOOP;\par
{\listtext\f1 39.\tab} END LOOP;\par
{\listtext\f1 40.\tab}END;\par

\pard 
{\listtext\f0 1.\tab}\jclisttab\tx1440\ls2\ilvl1\widctlpar\sa200\sl276\slmult1\cf2\expndtw15\f0\fs24 Goto\par

\pard\cbpat6 
{\listtext\f1 41.\tab}\jclisttab\tx1080\ls2\widctlpar\fi-360\cf0\expndtw0\f1\fs20 DECLARE\par
{\listtext\f1 42.\tab}  v_searched_number NUMBER  := 22;\par
{\listtext\f1 43.\tab}  v_is_prime        BOOLEAN\~:= true;\par
{\listtext\f1 44.\tab}BEGIN\par
{\listtext\f1 45.\tab}  FOR x IN\~2..v_searched_number-1 LOOP\par
{\listtext\f1 46.\tab}    IF v_searched_number MOD x = 0 THEN\par
{\listtext\f1 47.\tab}      dbms_output.put_line(v_searched_number|| ' is not a prime number..');\par
{\listtext\f1 48.\tab}      v_is_prime := false;\par
{\listtext\f1 49.\tab}      GOTO end_point;\par
{\listtext\f1 50.\tab}    END IF;\par
{\listtext\f1 51.\tab}  END LOOP;\par
{\listtext\f1 52.\tab}  IF\~v_is_prime THEN\~\par
{\listtext\f1 53.\tab}    dbms_output.put_line(v_searched_number|| ' is a prime number..');\par
{\listtext\f1 54.\tab}  END\~IF;\par
{\listtext\f1 55.\tab}<<end_point>>\par
{\listtext\f1 56.\tab}  dbms_output.put_line('Check complete..');\par
{\listtext\f1 57.\tab}END;\par

